{
  "parser": "babel-eslint",
  "extends": ["plugin:jsx-control-statements/recommended"],
  "rules": {
    "semi": [2, "always"],
    "space-before-function-paren": 0,
    "arrow-parens": 0,
    "react/jsx-no-undef": 0,
    "max-len": 0,
    "react/no-danger": 0,
    "jsx-control-statements/jsx-jcs-no-undef": 0
  }
}
